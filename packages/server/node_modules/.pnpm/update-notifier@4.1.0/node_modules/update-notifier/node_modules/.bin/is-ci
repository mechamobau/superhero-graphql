#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/bin.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules/is-ci/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/is-ci@2.0.0/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/node_modules:/Users/macbookpro15/Documents/ESTUDOS/node_modules:/Users/macbookpro15/Documents/node_modules:/Users/macbookpro15/node_modules:/Users/node_modules:/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules/is-ci/bin.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules/is-ci/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/update-notifier@4.1.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../is-ci/bin.js" "$@"
else 
  exec node  "$basedir/../../../is-ci/bin.js" "$@"
fi
