#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/nodemon@2.0.4/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/nodemon@2.0.4/node_modules/nodemon/bin/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/nodemon@2.0.4/node_modules/nodemon/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/nodemon@2.0.4/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/node_modules:/Users/macbookpro15/Documents/ESTUDOS/node_modules:/Users/macbookpro15/Documents/node_modules:/Users/macbookpro15/node_modules:/Users/node_modules:/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/nodemon/bin/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/nodemon/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else 
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
