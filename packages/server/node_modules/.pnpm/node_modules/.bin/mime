#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/cli.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/mime@1.6.0/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/node_modules:/Users/macbookpro15/Documents/ESTUDOS/node_modules:/Users/macbookpro15/Documents/node_modules:/Users/macbookpro15/node_modules:/Users/node_modules:/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/node_modules/mime/cli.js/node_modules:/Users/macbookpro15/Documents/ESTUDOS/GRAPHQL/superhero/packages/server/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else 
  exec node  "$basedir/../mime/cli.js" "$@"
fi
